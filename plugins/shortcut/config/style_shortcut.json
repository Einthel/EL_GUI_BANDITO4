{
  "QToolButton": {
    "border": "2px solid #555",
    "border-radius": "10px",
    "background-color": "#333",
    "color": "white",
    "padding": "5px"
  },
  "QToolButton:checked": {
    "background-color": "#4CAF50",
    "border": "2px solid #81C784"
  },
  "QToolButton:hover": {
    "background-color": "#444"
  },
  "QToolButton:pressed": {
    "background-color": "#2E7D32"
  }
}